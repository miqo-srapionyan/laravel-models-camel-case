{
  "name": "miqo/laravel-models-camel-case",
  "description": "Simply camelCase Laravel Model attributes and relationships",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "laravel-camel-case",
    "laravel-model-attributes"
  ],
  "autoload": {
    "psr-4": {
      "Miqo\\LaravelModelsCamelCase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Miqo\\LaravelModelsCamelCase\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Miqayel Srapionyan",
      "email": "miqosrapionyan@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7",
    "ext-mbstring": "*",
    "ext-ctype": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "10"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests"
  }
}
